            "type": "string",
            "nullable": true
          },
          "dataPublicacao": {
            "type": "string",
            "format": "date-time"
          },
          "dataFinal": {
            "type": "string",
            "format": "date-time"
          },
          "localProcesso": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
